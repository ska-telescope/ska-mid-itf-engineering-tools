{
    "manifest_version": 3,
    "name": "Bookmark Updater",
    "version": "1.0",
    "description": "Automatically updates specified bookmarks with new URLs based on defined namespaces.",
    "permissions": [
        "bookmarks"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
